html
  width 100%
  height 100%
  overflow: hidden
  background-color: black
  display flex
  justify-content: center
  align-items: center

body, #root
  width: 1280px
  height: 800px
  background: rgb(11, 40, 70)
  color: rgba(222, 222, 222, 0.9)
  font-family: sans-serif;
  overflow: hidden

*
  user-select: none

* > *
  box-sizing: border-box

.MuiSvgIcon-root
  cursor pointer
